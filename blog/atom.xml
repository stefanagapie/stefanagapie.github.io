---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Stefan Agapie's Blog</title>
	<link href="http://stefanagapie.github.io/blog/atom.xml" rel="self"/>
	<link href="http://stefanagapie.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://stefanagapie.github.io/blog</id>
	<author>
		<name>Stefan Agapie</name>
		<email>stefanagapie [a+t] g+ma+il [d+o+t] com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://stefanagapie.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://stefanagapie.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
